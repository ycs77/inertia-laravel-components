{
    "name": "ycs77/inertia-laravel-components",
    "description": "The Inertia.js Laravel & Vue components library",
    "keywords": ["laravel", "inertia"],
    "homepage": "https://github.com/ycs77/inertia-laravel-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "inertiajs/inertia-laravel": ">=0.3.6",
        "laravel/framework": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Inertia\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inertia\\ComponentsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
